set ns [new Simulator]
source tb_compat.tcl

set nodeA [$ns node]
set nodeB [$ns node]
set nodeC [$ns node]
set lan0 [$ns make-lan "$nodeC $nodeB $nodeA " 100Mb 0ms]
tb-set-node-os $nodeA UBUNTU14-64-STD
tb-set-node-os $nodeB UBUNTU14-64-STD
tb-set-node-os $nodeC UBUNTU14-64-STD
tb-set-sync-server $nodeA
tb-set-tarfiles /usr/local http://golang.org/dl/go1.4.2.linux-386.tar.gz
$ns rtproto Static
$ns run


